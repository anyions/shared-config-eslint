{
  "name": "@anyions/shared-config-eslint",
  "type": "module",
  "version": "1.0.7",
  "description": "AnyIons' shared eslint config presets with prettier",
  "author": {
    "name": "AnyIons",
    "email": "anyions@hotmail.com",
    "url": "https://github.com/anyions"
  },
  "license": "MIT",
  "homepage": "https://github.com/anyions/shared-config-eslint",
  "repository": {
    "url": "https://github.com/anyions/shared-config-eslint.git"
  },
  "bugs": {
    "url": "https://github.com/anyions/shared-config-eslint/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-typescript",
    "eslint-config-unocss",
    "eslint-config-vue",
    "eslint-config-prettier",
    "prettier"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "pnpm typegen && tsup --format esm,cjs --clean --dts",
    "bump": "bumpp && npm -r publish --access=public",
    "clean": "pnpm rimraf node_modules dist",
    "typegen": "esno scripts/typegen.ts",
    "lint": "pnpm eslint .",
    "lint:fix": "eslint . --cache --max-warnings 0 --fix",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "@anyions/node-pkg": "^1.0.5",
    "@eslint-react/eslint-plugin": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@unocss/eslint-config": "^0.63.6",
    "eslint": "^9.2.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.4.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.12.0",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "^3.3.3",
    "vue-eslint-parser": "^9.4.3"
  },
  "dependencies": {
    "@anyions/node-pkg": "*",
    "@eslint-react/eslint-plugin": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@unocss/eslint-config": "^0.63.6",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.4.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.12.0",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "jsonc-eslint-parser": "^2.4.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.9.0",
    "bumpp": "^9.8.1",
    "eslint": "^9.14.0",
    "eslint-typegen": "^0.3.2",
    "esno": "^4.8.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "8.3.5",
    "typescript": "^5.6.3"
  }
}
